#include <dt-bindings/clock/mdss-10nm-pll-clk.h>

&soc {
	mdss_mdp: qcom,mdss_mdp@ae00000 {
		compatible = "qcom,dpu-kms";
		reg = <0x0ae00000 0x81d40>,
		      <0x0aeb0000 0x2008>;
		reg-names = "mdp_phys",
			"vbif_phys";

		power-domains = <&clock_dispcc 0>;
		clocks =
			<&clock_gcc GCC_DISP_AHB_CLK>,
			<&clock_gcc GCC_DISP_AXI_CLK>,
			<&clock_dispcc DISP_CC_MDSS_AHB_CLK>,
			<&clock_dispcc DISP_CC_MDSS_AXI_CLK>,
			<&clock_dispcc DISP_CC_MDSS_MDP_CLK>,
			<&clock_dispcc DISP_CC_MDSS_VSYNC_CLK>;
		clock-names = "gcc_iface", "gcc_bus", "iface_clk",
				"bus_clk", "core_clk", "vsync_clk";
		clock-rate = <0 0 0 0 300000000 19200000 0>;
		clock-max-rate = <0 0 0 0 412500000 19200000 0>;

		/*dpu-vdd-supply = <&mdss_core_gdsc>;*/

		/* interrupt config */
		interrupt-parent = </*&pdc*/>;
		interrupts = <0 83 0>;
		interrupt-controller;
		#interrupt-cells = <1>;
		iommus = <&apps_smmu 0x880 0x8>,
			<&apps_smmu 0xc80 0x8>;

		#address-cells = <1>;
		#size-cells = <0>;

		#power-domain-cells = <0>;

		/* hw blocks */
		qcom,dpu-off = <0x1000>;
		qcom,dpu-len = <0x45C>;

		qcom,dpu-ctl-off = <0x2000 0x2200 0x2400
				     0x2600 0x2800>;
		qcom,dpu-ctl-size = <0xE4>;
		qcom,dpu-ctl-display-pref = "primary", "primary", "none",
					    "none", "none";

		qcom,dpu-mixer-off = <0x45000 0x46000 0x47000 0 0 0x4a000>;
		qcom,dpu-mixer-size = <0x320>;
		qcom,dpu-mixer-display-pref = "primary", "primary", "none",
					      "none", "none", "none";

		qcom,dpu-dspp-top-off = <0x1300>;
		qcom,dpu-dspp-top-size = <0xc>;

		qcom,dpu-dspp-off = <0x55000 0x57000 0x59000 0x5b000>;
		qcom,dpu-dspp-size = <0x17e0>;

		qcom,dpu-dest-scaler-top-off = <0x00061000>;
		qcom,dpu-dest-scaler-top-size = <0xc>;
		qcom,dpu-dest-scaler-off = <0x800 0x1000>;
		qcom,dpu-dest-scaler-size = <0x800>;

		qcom,dpu-wb-off = <0x66000>;
		qcom,dpu-wb-size = <0x2c8>;
		qcom,dpu-wb-xin-id = <6>;
		qcom,dpu-wb-id = <2>;
		qcom,dpu-wb-clk-ctrl = <0x3b8 24>;

		qcom,dpu-intf-off = <0x6b000 0x6b800
					0x6c000 0x6c800>;
		qcom,dpu-intf-size = <0x280>;

		qcom,dpu-intf-type = "dp", "dsi", "dsi", "dp";
		qcom,dpu-pp-off = <0x71000 0x71800
					  0x72000 0x72800 0x73000>;
		qcom,dpu-pp-slave = <0x0 0x0 0x0 0x0 0x1>;
		qcom,dpu-pp-size = <0xd4>;

		qcom,dpu-te2-off = <0x2000 0x2000 0x0 0x0 0x0>;
		qcom,dpu-cdm-off = <0x7a200>;
		qcom,dpu-cdm-size = <0x224>;

		qcom,dpu-dsc-off = <0x81000 0x81400 0x81800 0x81c00>;
		qcom,dpu-dsc-size = <0x140>;

		qcom,dpu-dither-off = <0x30e0 0x30e0 0x30e0 0x30e0 0x0>;
		qcom,dpu-dither-version = <0x00010000>;
		qcom,dpu-dither-size = <0x20>;

		qcom,dpu-sspp-type = "vig", "vig", "vig", "vig",
					"dma", "dma", "dma", "dma";

		qcom,dpu-sspp-off = <0x5000 0x7000 0x9000 0xb000
					0x25000 0x27000 0x29000 0x2b000>;
		qcom,dpu-sspp-src-size = <0x1c8>;

		qcom,dpu-sspp-xin-id = <0 4 8 12
					1 5 9 13>;
		qcom,dpu-sspp-excl-rect = <1 1 1 1
						1 1 1 1>;
		qcom,dpu-sspp-smart-dma-priority = <5 6 7 8 1 2 3 4>;
		qcom,dpu-smart-dma-rev = "smart_dma_v2";

		qcom,dpu-mixer-pair-mask = <2 1 6 0 0 3>;

		qcom,dpu-mixer-blend-op-off = <0x20 0x38 0x50 0x68 0x80 0x98
						0xb0 0xc8 0xe0 0xf8 0x110>;

		/* offsets are relative to "mdp_phys + qcom,dpu-off */
		qcom,dpu-sspp-clk-ctrl =
				<0x2ac 0>, <0x2b4 0>, <0x2bc 0>, <0x2c4 0>,
				 <0x2ac 8>, <0x2b4 8>, <0x2bc 8>, <0x2c4 8>;
		qcom,dpu-sspp-csc-off = <0x1a00>;
		qcom,dpu-csc-type = "csc-10bit";
		qcom,dpu-qseed-type = "qseedv3";
		qcom,dpu-sspp-qseed-off = <0xa00>;
		qcom,dpu-mixer-linewidth = <2560>;
		qcom,dpu-sspp-linewidth = <2560>;
		qcom,dpu-wb-linewidth = <4096>;
		qcom,dpu-mixer-blendstages = <0xb>;
		qcom,dpu-highest-bank-bit = <0x2>;
		qcom,dpu-ubwc-version = <0x200>;
		qcom,dpu-smart-panel-align-mode = <0xc>;
		qcom,dpu-panic-per-pipe;
		qcom,dpu-has-cdp;
		qcom,dpu-has-src-split;
		qcom,dpu-has-dim-layer;
		qcom,dpu-has-idle-pc;
		qcom,dpu-has-dest-scaler;
		qcom,dpu-max-dest-scaler-input-linewidth = <2048>;
		qcom,dpu-max-dest-scaler-output-linewidth = <2560>;
		qcom,dpu-max-bw-low-kbps = <6800000>;
		qcom,dpu-max-bw-high-kbps = <6800000>;
		qcom,dpu-min-core-ib-kbps = <2400000>;
		qcom,dpu-min-llcc-ib-kbps = <800000>;
		qcom,dpu-min-dram-ib-kbps = <800000>;
		qcom,dpu-dram-channels = <2>;
		qcom,dpu-num-nrt-paths = <0>;
		qcom,dpu-dspp-ad-version = <0x00040000>;
		qcom,dpu-dspp-ad-off = <0x28000 0x27000>;

		qcom,dpu-vbif-off = <0>;
		qcom,dpu-vbif-size = <0x1040>;
		qcom,dpu-vbif-id = <0>;
		qcom,dpu-vbif-memtype-0 = <3 3 3 3 3 3 3 3>;
		qcom,dpu-vbif-memtype-1 = <3 3 3 3 3 3>;

		qcom,dpu-vbif-qos-rt-remap = <3 3 4 4 5 5 6 6>;
		qcom,dpu-vbif-qos-nrt-remap = <3 3 3 3 3 3 3 3>;

		qcom,dpu-danger-lut = <0x0000000f 0x0000ffff 0x00000000
			0x00000000>;
		qcom,dpu-safe-lut-linear =
			<4 0xfff8>,
			<0 0xfff0>;
		qcom,dpu-safe-lut-macrotile =
			<10 0xfe00>,
			<11 0xfc00>,
			<12 0xf800>,
			<0 0xf000>;
		qcom,dpu-safe-lut-nrt =
			<0 0xffff>;
		qcom,dpu-safe-lut-cwb =
			<0 0xffff>;
		qcom,dpu-qos-lut-linear =
			<4 0x00000000 0x00000357>,
			<5 0x00000000 0x00003357>,
			<6 0x00000000 0x00023357>,
			<7 0x00000000 0x00223357>,
			<8 0x00000000 0x02223357>,
			<9 0x00000000 0x22223357>,
			<10 0x00000002 0x22223357>,
			<11 0x00000022 0x22223357>,
			<12 0x00000222 0x22223357>,
			<13 0x00002222 0x22223357>,
			<14 0x00012222 0x22223357>,
			<0 0x00112222 0x22223357>;
		qcom,dpu-qos-lut-macrotile =
			<10 0x00000003 0x44556677>,
			<11 0x00000033 0x44556677>,
			<12 0x00000233 0x44556677>,
			<13 0x00002233 0x44556677>,
			<14 0x00012233 0x44556677>,
			<0 0x00112233 0x44556677>;
		qcom,dpu-qos-lut-nrt =
			<0 0x00000000 0x00000000>;
		qcom,dpu-qos-lut-cwb =
			<0 0x75300000 0x00000000>;

		qcom,dpu-cdp-setting = <1 1>, <1 0>;

		qcom,dpu-qos-cpu-mask = <0x3>;
		qcom,dpu-qos-cpu-dma-latency = <300>;


		qcom,dpu-sspp-vig-blocks {
			qcom,dpu-vig-csc-off = <0x1a00>;
			qcom,dpu-vig-qseed-off = <0xa00>;
			qcom,dpu-vig-qseed-size = <0xa0>;
		};

		qcom,platform-supply-entries {
			#address-cells = <1>;
			#size-cells = <0>;

			qcom,platform-supply-entry@0 {
				reg = <0>;
				qcom,supply-name = "dpu-vdd";
				qcom,supply-min-voltage = <0>;
				qcom,supply-max-voltage = <0>;
				qcom,supply-enable-load = <0>;
				qcom,supply-disable-load = <0>;
			};
		};

		/* data and reg bus scale settings */
		qcom,dpu-data-bus {
			qcom,msm-bus,name = "mdss_dpu_mnoc";
			qcom,msm-bus,num-cases = <3>;
			qcom,msm-bus,num-paths = <2>;
			qcom,msm-bus,vectors-KBps =
			    <22 773 0 0>, <23 773 0 0>,
			    <22 773 0 6400000>, <23 773 0 6400000>,
			    <22 773 0 6400000>, <23 773 0 6400000>;
		};

		qcom,dpu-ebi-bus {
			qcom,msm-bus,name = "mdss_dpu_ebi";
			qcom,msm-bus,num-cases = <3>;
			qcom,msm-bus,num-paths = <1>;
			qcom,msm-bus,vectors-KBps =
			    <129 512 0 0>,
			    <129 512 0 6400000>,
			    <129 512 0 6400000>;
		};

		qcom,dpu-reg-bus {
			qcom,msm-bus,name = "mdss_reg";
			qcom,msm-bus,num-cases = <4>;
			qcom,msm-bus,num-paths = <1>;
			qcom,msm-bus,active-only;
			qcom,msm-bus,vectors-KBps =
				<1 590 0 0>,
				<1 590 0 76800>,
				<1 590 0 150000>,
				<1 590 0 300000>;
		};
	};

	dpu_rscc: qcom,dpu_rscc@af20000 {
		status = "disabled";
		cell-index = <0>;
		compatible = "qcom,dpu-rsc";
		reg = <0xaf20000 0x1c44>,
			<0xaf30000 0x3fd4>;
		reg-names = "drv", "wrapper";
		qcom,dpu-rsc-version = <1>;
		/*vdd-supply = <&mdss_core_gdsc>;*/
		clocks = <&clock_dispcc DISP_CC_MDSS_RSCC_VSYNC_CLK>,
			<&clock_dispcc DISP_CC_MDSS_RSCC_AHB_CLK>;
		clock-names = "vsync_clk", "iface_clk";
		clock-rate = <0 0>;

		qcom,dpu-dram-channels = <2>;

		/**
		 * mboxes = <&disp_rsc 0>;
		 * mbox-names = "disp_rsc";
		 */

		/* data and reg bus scale settings */
		qcom,dpu-data-bus {
			qcom,msm-bus,name = "disp_rsc_mnoc";
			qcom,msm-bus,active-only;
			qcom,msm-bus,num-cases = <3>;
			qcom,msm-bus,num-paths = <2>;
			qcom,msm-bus,vectors-KBps =
			    <20003 20515 0 0>, <20004 20515 0 0>,
			    <20003 20515 0 6400000>, <20004 20515 0 6400000>,
			    <20003 20515 0 6400000>, <20004 20515 0 6400000>;
		};

		qcom,dpu-llcc-bus {
			qcom,msm-bus,name = "disp_rsc_llcc";
			qcom,msm-bus,active-only;
			qcom,msm-bus,num-cases = <3>;
			qcom,msm-bus,num-paths = <1>;
			qcom,msm-bus,vectors-KBps =
			    <20001 20513 0 0>,
			    <20001 20513 0 6400000>,
			    <20001 20513 0 6400000>;
		};

		qcom,dpu-ebi-bus {
			qcom,msm-bus,name = "disp_rsc_ebi";
			qcom,msm-bus,active-only;
			qcom,msm-bus,num-cases = <3>;
			qcom,msm-bus,num-paths = <1>;
			qcom,msm-bus,vectors-KBps =
			    <20000 20512 0 0>,
			    <20000 20512 0 6400000>,
			    <20000 20512 0 6400000>;
		};
	};

	mdss_dsi0: qcom,mdss_dsi_ctrl0@ae94000 {
		compatible = "qcom,dsi-ctrl-hw-v2.2";
		label = "dsi-ctrl-0";
		cell-index = <0>;
		reg =   <0xae94000 0x400>,
			<0xaf08000 0x4>;
		reg-names = "dsi_ctrl", "disp_cc_base";
		interrupt-parent = <&mdss_mdp>;
		interrupts = <4 0>;
		vdda-1p2-supply = <&pm8998_l26>;
		clocks = <&clock_dispcc DISP_CC_MDSS_BYTE0_CLK>,
		<&clock_dispcc DISP_CC_MDSS_BYTE0_CLK_SRC>,
		<&clock_dispcc DISP_CC_MDSS_BYTE0_INTF_CLK>,
		<&clock_dispcc DISP_CC_MDSS_PCLK0_CLK>,
		<&clock_dispcc DISP_CC_MDSS_PCLK0_CLK_SRC>,
		<&clock_dispcc DISP_CC_MDSS_ESC0_CLK>;
		clock-names = "byte_clk", "byte_clk_rcg", "byte_intf_clk",
					"pixel_clk", "pixel_clk_rcg",
					"esc_clk";
		qcom,null-insertion-enabled;
		qcom,ctrl-supply-entries {
			#address-cells = <1>;
			#size-cells = <0>;

			qcom,ctrl-supply-entry@0 {
				reg = <0>;
				qcom,supply-name = "vdda-1p2";
				qcom,supply-min-voltage = <1200000>;
				qcom,supply-max-voltage = <1200000>;
				qcom,supply-enable-load = <21800>;
				qcom,supply-disable-load = <4>;
			};
		};
	};

	mdss_dsi1: qcom,mdss_dsi_ctrl1@ae96000 {
		compatible = "qcom,dsi-ctrl-hw-v2.2";
		label = "dsi-ctrl-1";
		cell-index = <1>;
		reg =   <0xae96000 0x400>,
